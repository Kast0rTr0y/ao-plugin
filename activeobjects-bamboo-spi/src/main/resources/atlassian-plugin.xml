<atlassian-plugin key="com.atlassian.activeobjects.bamboo.spi" name="${project.name}" system="false" pluginsVersion="2">
  <plugin-info>
    <description>${project.description}</description>
    <vendor name="Atlassian Software Systems" url="http://www.atlassian.com"/>
    <version>${project.version}</version>
  </plugin-info>

  <component key="dataSourceProvider" class="com.atlassian.activeobjects.bamboo.BambooDataSourceProvider" name="Data Source Provider" public="true">
    <interface>com.atlassian.activeobjects.spi.DataSourceProvider</interface>
  </component>

  <component key="executorServiceProvider" class="com.atlassian.activeobjects.bamboo.BambooExecutorServiceProvider" name="Executor Service Provider" public="true">
    <interface>com.atlassian.activeobjects.spi.ExecutorServiceProvider</interface>
  </component>

  <component key="dialectExtractor" class="com.atlassian.activeobjects.bamboo.hibernate.HibernateSessionDialectExtractor" name="Dialect Extractor"/>

  <component-import key="hibernateSessionFactory" interface="com.atlassian.bamboo.persistence3.PluginHibernateSessionFactory" name="Hibernate Session Factory"/>
</atlassian-plugin>
